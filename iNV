public GameObject[,,] InvImage = new GameObject[2,4,3];
    public Sprite[] Item;

    void Start() 
    {
        for (int i = 0; i < 2; i++)
        {
            for (int j = 0; j < 4; j++)
            {
                InvImage[i,j,0] = GameObject.Find("InvTile"+i.ToString()+"_"+j.ToString());

                Debug.Log(InvImage[i,j,0]);
            }
        }      
    }

